<!DOCTYPE html>
<html>
<head>
  <title>EPoll</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="//code.jquery.com/jquery-1.10.2.js"></script>
  <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
  <script src="/assets/dom.jsPlumb-1.7.6-min.js "></script>
  <script src="/assets/jquery.ui.touch-punch.min.js"></script>
</head>
<body>
	<header class="navbar navbar-inverse">
		<div class="container">
			<div class="navbar-header">
				<a href="/"><img src="assets/surrey.png"/><img src="assets/logo.png"/></a>
			</div>
			<ul class="nav navbar-nav">
				<li><%= link_to 'Add poll', new_poll_path %></li>
				<li><%= link_to 'Log out', new_poll_path %></li>
			</ul>
		</div>
	</header>

	<section id="content">
			<% flash.each do |key, value| %>
			<div class="alert alert-<%= key %>">
				<% value %>
			</div>
			<% end %>

			<div class="page-header">
				<h1><%= yield :page_header %></h1>
			</div>
			<%= yield %>
	</section>

</body>
</html>
